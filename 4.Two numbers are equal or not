#include<iostream>
using namespace std;
int main()
{
    int n1,n2;
    cout<<"Enter the number: ";
    cin>>n1;
    cout<<"Enter the number: ";
    cin>>n2;
    if(n1==n2)
    {
        cout<<n1<<" is equal to "<<n2<<endl;
    }
    else
    {
        cout<<n1<<" is not equal to "<<n2<<endl;
    }
    return 0;
}
